{
  "name": "e2e",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint . --fix",
    "test": "jest --testPathPatterns=./src/",
    "coverage": "jest --coverage",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "http-server dist",
    "e2e": "jest --testPathPatterns=./e2e/ --runInBand",
    "doc": "jsdoc -c jsdoc.conf.json",
    "preshow:doc": "npm run doc",
    "show:doc": "live-server doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiotobat/e2e.git"
  },
  "author": "Spartak",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@eslint/js": "^9.31.0",
    "babel-jest": "^30.0.4",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "jsdoc": "^4.0.4",
    "jsdom": "^26.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.6.2",
    "puppeteer": "^24.15.0",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "core-js": "^3.44.0",
    "puppeteer": "^24.15.0"
  }
